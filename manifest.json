{
	"manifest_version": 2,
	"version": "1.0",
	"name": "First-Extension",
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"main.js"
			]
		}
	],
	"web_accessible_resources": [
		"pp.jpg"
	],
	"icons": {
		"48": "xmen.png",
		"96": "xmen.png"
	},
	"browser_action": {
		"default_icon": {
			"19": "xmen.png",
			"38": "xmen.png"
		}
	},
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"permissions": [
		"tabs"
	]
}